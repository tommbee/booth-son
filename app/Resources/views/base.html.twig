    <head>
        <title>B. Booth & Son | {% block title %}{% endblock %}</title>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" type="image/x-icon" href="{{ asset('images/favicon.ico') }}" />

        <noscript>
            <link rel="stylesheet" href="{{ asset('build/css/main.css') }}">
        </noscript>

        <style type="text/css">
            .loading {
                position: fixed;
                width: 100%;
                height: 100%;
                background: white;
                z-index: 100;
                top: 0;
                left: 0;
                opacity: 1;
                background: url({{ asset('images/ajax-loader.gif') }}) no-repeat center center black;
                background-size: 24px 24px;
                transition: opacity .3s linear;
            }
            .loading.hide {
                opacity: 0;
            }
            .loading.hide.back {
                z-index: -9999;
            }
            .is-exiting .loading.hide.back {
                z-index: 1000;
                opacity: 1;
                transition-delay: 500ms;
            }
            hr.hr {
                transform: scaleX(0);
            }

            .transition {
                position: relative;
                transition: all 600ms cubic-bezier(0.835, -0.15, 0.125, 0.965);
                backface-visibility: hidden;
                transition-delay: 800ms;
            }
            .delay {
                transition-delay: 1000ms;
            }
            .delay-two {
                transition-delay: 1200ms;
            }

            .delay-three {
                transition-delay: 1400ms;
            }
            .slide-opacity {
                opacity: 0;
            }
            .slide-left {
                transform: translate(-1rem, 0);
                opacity: 0;
            }
            .slide-right {
                transform: translate(1rem, 0);
                opacity: 0;
            }
            .loaded .slide-opacity {
                opacity: 0.4;
            }
            .loaded .slide-left {
                transform: translate(0, 0rem);
                opacity: 1;
            }
            .loaded .slide-right {
                transform: translate(0, 0rem);
                opacity: 1;
            }
            .loaded.is-exiting .slide-left {
                transform: translate(-1rem, 0) !important;
                opacity: 0;
                transition-delay: 100ms !important;
            }
            .loaded.is-exiting .slide-left.delay {
                transition-delay: 0s !important;
            }
            .loaded.is-exiting .slide-right {
                transform: translate(1rem, 0) !important;
                opacity: 0;
                transition-delay: 0s !important;
            }
        </style>

    </head>

    <body>
        <nav>
            <div class="burger">
                <a>
                    <span></span>
                    <span>open/close menu</span>
                    <span></span>
                </a>
            </div>
            <div class="nav_holder">
                <div class="row">
                    <div class="contain">
                        <a href="{{ path("homepage") }}" class="one"><span>Home</span></a>
                        <a href="{{ path("about") }}" class="two"><span>About</span></a>
                        <span class="spacer"></span>
                        <a href="{{ path("projects") }}" class="three"><span>Projects</span></a>
                        <a href="{{ path("contact") }}" class="four"><span>Contact</span></a>
                    </div>
                    {% if is_granted("IS_AUTHENTICATED_REMEMBERED") %}
                        <a class="logout-button btn" href="{{ path('fos_user_security_logout') }}">logout</a>
                    {% endif %}
                </div>
            </div>
            <div class="row">
                <div class="columns small-24 aligncenter">
                    <a class="logo" href="/">B.Booth <span class="amp">&amp;</span> Son</a>
                    {#<a class="contact-but cta floatright">GET IN TOUCH</a>#}
                </div>
            </div>
            <div class="row">
                <div class="columns small-offset-1 small-22 medium-offset-9 medium-6">
                    <hr class="hr" />
                    <div class="mobile_menu">
                        <a href="{{ path("homepage") }}" class="one"><span>Home</span></a>
                        <a href="{{ path("about") }}" class="two"><span>About</span></a>
                        <a href="{{ path("projects") }}" class="three"><span>Projects</span></a>
                        <a href="{{ path("contact") }}" class="four"><span>Contact</span></a>
                    </div>
                </div>
            </div>
            <div id="main" class="aligncenter">
                <div class="row" style="height: 100%;position:relative;">
                    <h1>
                        Property Construction &amp; Renovation<br/>
                        {#<span>Quality Guaranteed</span>#}
                    </h1>
                    {#<a class="cta">View recent projects</a>#}
                </div>
            </div>
        </nav>
        <div class="loading"></div>
        {% block body %}

        {% endblock %}
        <footer>
            <p>&copy; B. Booth &amp; Son <script>document.write(new Date().getFullYear())</script></p>
        </footer>
    </body>
